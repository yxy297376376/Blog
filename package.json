{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "5.0.0",
    "aplayer": "^1.10.1",
    "classnames": "^2.3.2",
    "i18next": "^22.0.6",
    "mockjs": "^1.1.0",
    "react": "17.x",
    "react-aplayer": "^1.0.0",
    "react-dom": "17.x",
    "react-fast-marquee": "^1.3.5",
    "react-i18next": "^12.0.0",
    "react-particles": "^2.5.3",
    "tsparticles": "^2.5.3",
    "tsparticles-engine": "^2.5.2",
    "tsparticles-preset-confetti": "^2.5.3",
    "umi": "^3.5.32",
    "video-react": "^0.16.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.35",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
